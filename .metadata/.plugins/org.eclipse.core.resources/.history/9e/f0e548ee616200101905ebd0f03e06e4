package Collections1;

import java.util.Vector;

public class vectorColl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Vector v1=new Vector();
		Vector v2=new Vector();
		v1.add("Naren");
		v1.add(42);
		v1.add(0,"akurathi");
		v1.add("Vindhya");
		v1.add(3);
		v1.add(0,"akurathi");
		v1.add("Lalitha varshini");
		v1.add(4);
		v1.add(0,"akurathi");
		v1.add("Hemalatha");
		v1.add(35);
		v1.add(0,"akurathi venkata");
		System.out.println("size before removing - "+v1.size());
		v1.remove(0);
		System.out.println("size aftee remove - "+v1.size());
		v1.add("Naren");
		v1.addAll(v1);
		v1.add(42);
		v1.add(null);
		
		System.out.println(v1);
		System.out.println(v1.capacity());
	}

}
