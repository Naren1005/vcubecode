package Collections1;

import java.util.Vector;

public class vectorColl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Vector v1=new Vector();
		Vector v2=new Vector();
		v1.add("Naren");
		v1.add(42);
		v1.add(0,"akurathi");
		v1.add("Vindhya");
		v1.add(3);
		v1.add(0,"akurathi");
		v1.add("Lalitha varshini");
		v1.add(4);
		v1.add(0,"akurathi");
		v1.add("Hemalatha");
		v1.add(35);
		v1.add(0,"akurathi venkata");
		System.out.println("size before removing - "+v1.size());
		v1.remove(0);
		System.out.println("size aftee remove - "+v1.size());
		v1.add("Naren");
		v2.addAll(v1);
		v1.add(42);
		v1.add(null);
		System.out.println("the element at index 5 is :"+v2.elementAt(5));
		System.out.println("comparing 2 vectors"+v2.equals(v1));
		System.out.println("checking for vindya in the v1 collection  -" + v1.contains("Vindhya"));
		
		System.out.println("the first element of the v1 is : "+  v1.firstElement());
		System.out.println("the first element of the v1 is : "+  v1.lastElement());
		System.out.println("v1 variables are "+v1);
		System.out.println("v2 variables are" +v2);
		v2.clear();
		System.out.println("v2 variables after clearing" +v2);
		System.out.println(v1.get(6));
		
		System.out.println(v1.capacity());
	}

}
